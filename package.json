{
  "name": "pi-agent-extensions",
  "version": "0.1.0",
  "description": "A collection of pi coding agent extensions",
  "type": "module",
  "workspaces": [
    "fetch"
  ],
  "keywords": ["pi-package"],
  "author": "rytswd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rytswd/pi-agent-extensions.git"
  },
  "dependencies": {
    "@marckrenn/pi-sub-core": "*",
    "@marckrenn/pi-sub-bar": "*"
  },
  "peerDependencies": {
    "@mariozechner/pi-ai": "*",
    "@mariozechner/pi-agent-core": "*",
    "@mariozechner/pi-coding-agent": "*",
    "@mariozechner/pi-tui": "*",
    "@sinclair/typebox": "*"
  },
  "pi": {
    "extensions": [
      "direnv",
      "fetch",
      "questionnaire", 
      "slow-mode",
      "node_modules/@marckrenn/pi-sub-core",
      "node_modules/@marckrenn/pi-sub-bar"
    ]
  }
}
